<Page x:Class="LegistOS.Stranici.PolzovatelskoeSoglashenie"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LegistOS.Stranici"
      mc:Ignorable="d" 
      d:DesignHeight="400" d:DesignWidth="880"
      Title="Пользовательское соглашение">
    <Grid Margin="10">
        <TextBox x:Name="TBPolzSogl" Width="850" Height="350" 
                 Text="Пользовательское соглашение"
                 BorderBrush="{x:Null}" IsReadOnly="True"
                 Style="{StaticResource TBOsn}" TextWrapping="Wrap"  />
    </Grid>
</Page>
